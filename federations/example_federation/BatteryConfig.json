{
  "name": "Battery",
  "core_type": "zmq",
  "core_name": "battery_core",
  "log_level": "warning",
  "period": 60,
  "offset": 0.0,
  "uninterruptible": false,
  "terminate_on_error": true,
  "wait_for_current_time_update": true,
  "publications": [
    {
      "global": true,
      "key": "Battery/Battery_dummy_0/current",
      "type": "double",
      "unit": "A"
    },
    {
      "global": true,
      "key": "Battery/Battery_dummy_1/current",
      "type": "double",
      "unit": "A"
    }
  ],
  "subscriptions": [
    {
      "global": true,
      "key": "Charger/Charger_dummy_0/voltage",
      "type": "double",
      "unit": "V"
    },
    {
      "global": true,
      "key": "Charger/Charger_dummy_1/voltage",
      "type": "double",
      "unit": "V"
    }
  ]
}
